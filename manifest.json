{
  "name": "Eventry",
  "version": "1.0",
  "description": "Log your hours easily!",
  "manifest_version": 2,
  "background": {
  "scripts": ["js/background.js"],
  "persistent": false
  },
  "browser_action": {
    "default_title": "Eventry",
    "default_popup": "pages/index.html"
  },
  "permissions": [
    "storage",
    "*://*.10000ft.com/*"
  ],
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com; object-src 'self'"
}
